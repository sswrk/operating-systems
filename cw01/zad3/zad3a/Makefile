CC = gcc -Wall -Wl,--no-as-needed -ldl

all: static shared clean

static:
	$(CC) -c library.c
	ar rcs library.a library.o

shared:
	$(CC) -fPIC -c library.c
	$(CC) -fPIC -shared -o library.so library.o

clean:
	rm -f *.o

zad3_static:
	make static
	$(CC) -o test main.c library.a -I .
	# works :)
	make tests

zad3_dynamic:
	make shared
	$(CC) -o test main_dynamic.c -I .
	# works :)
	make tests

zad3_shared:
	#export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/lib
	make shared
	$(CC) -o test main.c -L. library.so -I .
	# works :)
	make tests

zad3_all:
	echo Z wykorzystaniem bibliotek statycznych: > raport3a.txt
	make zad3_static
	echo Z wykorzystaniem bibliotek dynamicznych: | tee -a raport3a.txt
	make zad3_dynamic
	echo Z wykorzystaniem bibliotek dzielonych: | tee -a raport3a.txt
	make zad3_shared

tests:
	echo Testy: | tee -a raport3a.txt

	echo Mala ilosc porownan: | tee -a raport3a.txt

	echo Zapisywanie: | tee -a raport3a.txt

	echo 10% podobienstwa: | tee -a raport3a.txt
	./test 3 compare_pairs txt_files/10similarity/1a.txt:txt_files/10similarity/1b.txt txt_files/10similarity/2a.txt:txt_files/10similarity/2b.txt txt_files/10similarity/3a.txt:txt_files/10similarity/3b.txt  | tee -a raport3a.txt
	echo 50% prawdopodobienstwa: | tee -a raport3a.txt
	./test 3 compare_pairs txt_files/50similarity/1a.txt:txt_files/50similarity/1b.txt txt_files/50similarity/2a.txt:txt_files/50similarity/2b.txt txt_files/50similarity/3a.txt:txt_files/50similarity/3b.txt | tee -a raport3a.txt
	echo 90% prawdopodobienstwa: | tee -a raport3a.txt
	./test 3 compare_pairs txt_files/90similarity/1a.txt:txt_files/90similarity/1b.txt txt_files/90similarity/2a.txt:txt_files/90similarity/2b.txt txt_files/90similarity/3a.txt:txt_files/90similarity/3b.txt | tee -a raport3a.txt
	
	echo Usuwanie: | tee -a raport3a.txt

	echo 10% podobienstwa: | tee -a raport3a.txt
	./test 3 compare_pairs txt_files/10similarity/1a.txt:txt_files/10similarity/1b.txt txt_files/10similarity/2a.txt:txt_files/10similarity/2b.txt txt_files/10similarity/3a.txt:txt_files/10similarity/3b.txt remove_block 0 remove_block 0 remove_block 0 | tee -a raport3a.txt
	echo 50% prawdopodobienstwa: | tee -a raport3a.txt
	./test 3 compare_pairs txt_files/50similarity/1a.txt:txt_files/50similarity/1b.txt txt_files/50similarity/2a.txt:txt_files/50similarity/2b.txt txt_files/50similarity/3a.txt:txt_files/50similarity/3b.txt remove_block 0 remove_block 0 remove_block 0 | tee -a raport3a.txt
	echo 90% prawdopodobienstwa: | tee -a raport3a.txt
	./test 3 compare_pairs txt_files/90similarity/1a.txt:txt_files/90similarity/1b.txt txt_files/90similarity/2a.txt:txt_files/90similarity/2b.txt txt_files/90similarity/3a.txt:txt_files/90similarity/3b.txt remove_block 0 remove_block 0 remove_block 0 | tee -a raport3a.txt
	
